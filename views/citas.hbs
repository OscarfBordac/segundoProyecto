    <header><h1>Centro Medico</h1></header>
    <main class="tabla-container">
        <table  border="solid" >
            <h2>LISTADO DE MEDICOS</h2>
            <thead>
                <tr>
                    <th>Fecha cita</th>
                    <th>Paciente</th>
                    <th>Telefono paciente</th>
                    <th>Especialidad</th>
                    <th>Consultorio</th>
                    <th>Medico</th>

                    
                </tr>
            </thead>

            <tbody >
                <!-- Se agrega el contenido dinamicamente por medio de handlebars -->
                {{#each resultado}}
                <tr>
                    <td>{{formatoFecha fecha_cita}}</td>
                    <td>{{nombres}} {{apellidos}}</td>
                    <td>{{telefono}}</td>
                    <td>{{especialidad}}</td>
                    <td>{{consultorio}}</td>
                    <td>{{nombresMedico}} {{apellidosMedico}}</td>

                    
                </tr>
                {{/each}}
            </tbody>
        </table>
    </main>

    <br>
